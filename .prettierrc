{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "overrides": [
    { "files": "*.ts", "options": { "parser": "typescript" } },
    { "files": "*.tsx", "options": { "parser": "typescript" } },
    { "files": "*.js", "options": { "parser": "babel" } },
    { "files": "*.jsx", "options": { "parser": "babel" } }
  ]
}
